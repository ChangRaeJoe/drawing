<div class='boardContainer boxing'>
    <!-- list add -->
</div>
<div class="boardContainer">
    <div class="btnRight btnMagin">
        <button onClick="location.href='/board/createDraw.html'">
            Write
        </button>
    </div>
    <div class="page btnCenter">
        <% if(active['prev']) { %>
            <div><a href="/board/imgboard.html?page=<%= frontPage-PAGE_SIZE %>">prev</a></div>
        <% } else { %>
            <div>prev</div>
        <% } %>

        <% for(let i=frontPage; i<=lastPage; i++) { %>
            <button class="btnMaginSide" >
                <%- i %>
            </button>
        <% } %>

        <% if(active['next']) { %>
            <div><a href="/board/imgboard.html?page=<%= lastPage+1 %>">next</a></div>
        <% } else { %>
            <div>next</div>
        <% } %>
    </div>

</div>


